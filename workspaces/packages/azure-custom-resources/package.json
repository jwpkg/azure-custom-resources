{
  "name": "azure-custom-resources",
  "packageManager": "yarn@4.2.2",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^24.5.2",
    "jest": "^29.7.0",
    "jest-util": "^29.7.0",
    "ts-jest": "^29.4.4",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@azure/functions": "^4.5.0",
    "bs58": "^6.0.0"
  },
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -f tsconfig.tsbuildinfo && tsc --noEmit false",
    "copy-readme": "cp ../../../README.md .",
