    "postpack": "rm -rf lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jwpkg/azure-custom-resources",
    "directory": "workspaces/packages/azure-custom-resources"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "version": "0.0.0"
}
